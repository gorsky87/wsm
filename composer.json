{
    "name": "droptica/droopler-project",
    "description": "Project template for Droopler with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.droopler.com",
    "support": {
        "docs": "https://www.droopler.com/developers",
        "issues": "https://www.drupal.org/project/issues/droopler?categories=All"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "defuse/php-encryption": "^2.2",
        "droptica/droopler": "8.3.*",
        "drupal/admin_toolbar": "3.0",
        "drupal/avatars": "^1.1",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/config_sync": "^2.0@beta",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/entity_type_clone": "^1.6",
        "drupal/eu_cookie_compliance": "^1.14",
        "drupal/field_permissions": "^1.1",
        "drupal/geofield": "^1.22",
        "drupal/geofield_map": "^2.67",
        "drupal/leaflet": "^2.1",
        "drupal/leaflet_more_maps": "^2.1",
        "drupal/mail_login": "^2.4",
        "drupal/r4032login": "^2.1",
        "drupal/recaptcha": "^3.0",
        "drupal/responsive_views_grid": "^1.1",
        "drupal/simplelogin": "^6.0",
        "drupal/social_login": "^2.89",
        "drupal/unique_entity_title": "^1.2",
        "drupal/url_restrictions": "^2.0",
        "drush/drush": "~9.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^4.0.0-rc1",
        "kint-php/kint": "^3.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library", "type:git", "type:bower-asset", "type:npm-asset"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
